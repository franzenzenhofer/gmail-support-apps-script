{
  "name": "gmail-support-apps-script",
  "version": "1.0.0",
  "description": "Complete Gmail Support System - 100% Google Apps Script",
  "scripts": {
    "login": "clasp login",
    "logout": "clasp logout",
    "create": "clasp create --title 'Gmail Support System' --type webapp",
    "clone": "clasp clone",
    "pull": "clasp pull",
    "push": "clasp push",
    "push:watch": "clasp push --watch",
    "build": "node build.js",
    "deploy": "./deploy.sh",
    "deploy:prod": "npm run push && clasp deploy --description 'Production deployment'",
    "deploy:bundle": "npm run build && clasp push dist/bundled.gs",
    "undeploy": "clasp undeploy",
    "logs": "clasp logs",
    "logs:watch": "clasp logs --watch",
    "open": "clasp open",
    "status": "clasp status",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:gas": "clasp run runComprehensiveTests",
    "version": "clasp version",
    "lint": "eslint **/*.gs",
    "format": "prettier --write **/*.{gs,html,json,md}",
    "setup": "npm install && npm run login",
    "dev": "npm run push:watch",
    "dev:test": "npm run test:watch",
    "run": "clasp run",
    "run:function": "clasp run --function"
  },
  "keywords": [
    "gmail",
    "support",
    "customer-service",
    "google-apps-script",
    "automation",
    "ai",
    "gemini"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@google/clasp": "^2.4.2",
    "@types/google-apps-script": "^1.0.56",
    "babel-jest": "^29.5.0",
    "eslint": "^8.45.0",
    "eslint-plugin-googleappsscript": "^1.0.5",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}